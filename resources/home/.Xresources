*.color15:      #f2f0ec


Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.hintstyle: hintfull

! urxvt options
URxvt.perl-ext-common: default,matcher
URxvt.geometry: 100x36
URxvt.cursorBlink: true
URxvt.scrollBar: false
!URxvt.internalBorder: 20
URxvt.font: \
	xft:Hack Nerd Font:style:Regular:pixelsize=15, \
	xft:Source Han Sans:pixelsize=15, \
	xft:Symbols Nerd Font:style=2048-em \
URxvt.allow_bold: true
! true transparency
URxvt.depth: 32
URxvt.background: [95]#111111
! fix scrollback when using a pager
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0
!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D
! clickable urls
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
! open last url
URxvt.keysym.C-Delete: perl:matcher:last
! list open urls (max 10)
URxvt.keysym.M-Delete: perl:matcher:list
! url color
URxvt.matcher.rend.0: Uline Bold fg5
