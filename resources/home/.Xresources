! special
*.foreground:   #d0d0d0
*.cursorColor:  #d0d0d0
*.background:   #111111

! black
*.color0:       #151515
*.color8:       #505050

! red
*.color1:       #F25054
*.color9:       #F25054

! green
*.color2:       #4BCC4B
*.color10:      #4BCC4B

! yellow
*.color3:       #FFC34C
*.color11:      #FFC34C

! blue
*.color4:       #4187CC
*.color12:      #4187CC

! magenta
*.color5:       #CF66CF
*.color13:      #CF66CF

! cyan
*.color6:       #42CCCC
*.color14:      #42CCCC

! white
